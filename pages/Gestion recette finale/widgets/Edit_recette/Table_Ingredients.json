      "onOptionChange": "{{\n  (() => {\n    const updatedRow = Table_Ingredients.updatedRow;\n    const data = [...appsmith.store.editRecetteFinaleData];\n\n    const index = data.findIndex(row => row.id_composant === updatedRow.id_composant);\n    if (index !== -1) {\n      const selectedUnite = data[index].unite_options.find(u => u.id_unite === updatedRow.id_unite);\n      if (selectedUnite) {\n        data[index] = {\n          ...data[index],\n          id_unite: selectedUnite.id_unite,\n          unite: selectedUnite.nom_unite\n        };\n        storeValue(\"editRecetteFinaleData\", data);\n      }\n    }\n  })()\n}}\n",
